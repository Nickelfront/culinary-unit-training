<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>

<AnchorPane prefHeight="590.0" prefWidth="635.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.ClientsController">
   <children>
      <HBox prefHeight="483.0" prefWidth="721.0" AnchorPane.leftAnchor="50.0" AnchorPane.topAnchor="30.0">
         <children>
            <TabPane prefHeight="44.0" prefWidth="1040.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Клиенти">
                     <content>
                        <TableView fx:id="clientsTable" prefHeight="423.0" prefWidth="295.0" snapToPixel="false">
                          <columns>
                            <TableColumn fx:id="idColumn" prefWidth="79.0" resizable="false" text="ID" />
                            <TableColumn fx:id="firstNameColumn" minWidth="-1.0" prefWidth="116.0" resizable="false" text="Име" />
                              <TableColumn fx:id="lastNameColumn" minWidth="-1.0" prefWidth="133.0" resizable="false" text="Фамилия" />
                              <TableColumn fx:id="ageColumn" minWidth="-1.0" prefWidth="64.0" resizable="false" text="Възраст" />
                              <TableColumn fx:id="phoneColumn" minWidth="-1.0" prefWidth="117.0" resizable="false" text="Телефон" />
                              <TableColumn fx:id="emailColumn" minWidth="-1.0" prefWidth="75.0" resizable="false" text="E-Mail" />
                          </columns>
                        </TableView>
                     </content>
                  </Tab>
                <Tab text="Нов таб" />
              </tabs>
               <HBox.margin>
                  <Insets />
               </HBox.margin>
            </TabPane>
         </children>
      </HBox>
   </children>
</AnchorPane>
